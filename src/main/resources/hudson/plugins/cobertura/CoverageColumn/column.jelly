<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core">
  <td>
    <j:if test="${!it.hasUrl(job)}">
      ${it.getCoverage(job)}
    </j:if>
    <j:if test="${it.hasUrl(job)}">
      <a href="${jobBaseUrl}${job.shortUrl}${it.getUrl(job)}">${it.getCoverage(job)}</a>
    </j:if>
  </td>
</j:jelly>
